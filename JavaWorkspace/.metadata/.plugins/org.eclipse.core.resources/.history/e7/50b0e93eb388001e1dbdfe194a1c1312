package essentials;

public class Class_Product_Test {
	public static void main(String[] args) {
		/*
		 * This is where i'm going to test the linking between classes.
		 * 
		 * */
		Class_Product p1 = new Class_Product();
		p1.name = "Lenovo Ideapad";
		p1.price = 1.499;
		p1.discount = 1.5;
		
		double finalPrice = p1.price - (p1.discount * p1.price);
		
	}
}
