package fundamentals;
import java.util.Scanner;

public class WhileCounter {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
	
		String answer = sc.next();
		
		int counter = 0;
		
		while(counter < 10) {
			counter++;
			System.out.print(counter);
		}
	
	
	
	
	
	
	
	
	
		sc.close();
	}
}
