package fundamentals;

public class Switch {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);

		String concept = "";
		
		
		switch(points) {
		case 10: case  9:
			concept = "A";
		break;
		
		case 8: case  7:
			concept = "B";
		break;
		
		case 6: case  5:
			concept = "C";
		break;
		
		case 4: case  3:
			concept = "D";
		break;
		
		case 2: case  1:
			concept = "E";
		break;
		case 0:
			concept = "F";
			break;
		default:
			System.out.println("Invalid value.");
		}
		
		sc.close();
		
		
		
		
	}
	}

