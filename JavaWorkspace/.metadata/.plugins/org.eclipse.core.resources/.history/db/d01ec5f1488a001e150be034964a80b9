package exercises;

public class Date_Class {
	int day;
	int month;
	int year;
	
	Date_Class(){
	}
	
	Date_Class(int initialDay, int initialMonth, int initialYear){
		day = initialDay;
		month = initialMonth;
		year = initialYear;
	}
	
	String formatDate() {
		return String.format("%d/%d/%d", day, month, year);
	}
}
